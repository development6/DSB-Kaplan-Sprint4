        function LoadChart() {
            var chartType = parseInt($("#rblChartType input:checked").val());
            var ingresoKG = [];
            var egresoKG = [];
            var datachart = {
                labels: ["KG MUSC", "KG Grasa"],
                datasets: [
                    {
                        label: "Ingreso",
                        backgroundColor: "rgba(90,193,208,0.3)",
                        borderColor: "rgba(151,187,205,0.7)",
                        borderWidth: 1,
                        hoverBackgroundColor: "rgba(151,187,205,1)",
                        hoverBorderColor: "rgba(151,187,205,0.5)",
                        data: ingresoKG
                    },
                    {
                        label: "Egreso",
                        backgroundColor: "rgba(215,220,67,0.3)", //rgba(150,236,136,0.7)
                        borderColor: "rgba(220,220,220,0.7)",
                        borderWidth: 1,
                        hoverBackgroundColor: "rgba(220,220,220,1)",
                        hoverBorderColor: "rgba(220,220,220,0.5)",
                        data: egresoKG
                    }
                ]
            };
            $.ajax({
                type: "POST",
                url: "/Home/AjaxMethod",
                data: "{Paciente: '" + $("#ddlPaciente").val() + "'}",
                contentType: "application/json; charset=utf-8",
                dataType: "text",
                success: function (response) {
                    $("#dvChartKG").html("");
                    
                    $("#dvMensajeKG").html("");
                    //$("#dvTabla").html("");

                    //window.alert(response);
                    if (response[9] == ',') { //response[17]==','
                        var div = $("<div />");
                        //div.css("font-size", "18px");
                        div.html("<p class='caja'>Este paciente no posee datos para mostrar</p>");
                        $("#dvMensajeKG").append(div);
                    };


                    //$("#dvLegend").html("");
                    //$("#dvTabla").html("");
                    var data = eval(response);
                    //var data = (eval("(" + response.d + ")"));
                    var el = document.createElement('canvas');
                    $("#dvChartKG")[0].appendChild(el);


                    //window.alert(data);
                    //Fix for IE 8
                    if ($.browser.msie && $.browser.version == "8.0") {
                        G_vmlCanvasManager.initElement(el);
                    }
                    
                    for (var i = 0; i < data.length; i++) {
                        ingresoKG.push(data[i].value0, data[i].value1);
                        egresoKG.push(data[i].value9, data[i].value10);
                    }

                    //console.log(ingreso);
                    //window.alert(ingreso);

                  

                    var ctx = el.getContext('2d');
                    var userStrengthsChart;

                    switch (chartType) {
                        case 1:
                            userStrengthsChart = new Chart(ctx, {
                                type: 'bar',
                                data: datachart,
                                options: {
                                    title: {
                                        display: true,
                                        text: "Grafico KG"
                                    },
                                    scales: {
                                        xAxes: [{
                                            barPercentage: 0.6
                                        }],
                                        yAxes: [
                                            {
                                                ticks: {
                                                    //fontFamily: "Roboto Mono",
                                                    //fontColor: "#556F7B",
                                                    //fontStyle: "bold",
                                                    beginAtZero: true,
                                                    //maxTicksLimit: 5,
                                                    //padding: 20
                                                },
                                                scaleLabel: {
                                                    display: true,
                                                    labelString: 'Kg'
                                                },
                                                gridLines: {
                                                    //drawTicks: false,
                                                    //display: false,
                                                    //drawBorder: false
                                                }
                                            }
                                        ],
                                    }
                                }
                            });
                            break;
                        case 2:
                            userStrengthsChart = new Chart(ctx, {
                                type: 'line',
                                data: datachart,
                                options: {
                                    title: {
                                        display: true,
                                        text: "Grafico KG"
                                    },
                                    
                                    /*scale: {
                                        ticks: {
                                            beginAtZero: true,
                                        }
                                    }*/
                                }
                            });
                            break;

                    }
                },
                failure: function (response) {
                    alert('Ocurrió un error.');
                },

            });
        }







